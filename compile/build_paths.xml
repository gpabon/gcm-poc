<project basedir=".">
    <property name="base.dir" 			value=".."/> 
    <property name="compile.dir"        value="${base.dir}/compile" /> 
    <property name="src.dir"   value="${base.dir}/src" /> 
    <property name="lib.dir"            value="${base.dir}/dist/lib" /> 
    <property name="classes.dir"  	    value="${base.dir}/classes" /> 
    <property name="source"				value="1.5"/>
    <property name="debug"				value="on"/> 
    <property name="log4j.properties"   value="${compile.dir}/log4j.properties" /> 
    <property name="package.path"  value="test/gpabon/gcmpoc" />

    <path id="compile.classpath">
        <fileset dir="${lib.dir}">
            <include name="**/*.jar"/>
        </fileset>
    </path>

</project>
